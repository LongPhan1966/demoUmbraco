@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.LoginRegister>
@using ContentModels = Umbraco.Web.PublishedModels;
@{
    Layout = "WebBaseTemplates.cshtml";
    var home = (Home)Umbraco.Content(1065);
}

<link href="~/css/regiter.css" rel="stylesheet" />
@Umbraco.RenderMacro("RegisterMember")
@Umbraco.RenderMacro("Login")
@if (Members.GetCurrentLoginStatus().IsLoggedIn && Request.UrlReferrer != null)
{
    Response.Redirect(home.Url());
}
